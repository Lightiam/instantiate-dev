# Production Environment Variables for instantiate.dev
# Configure these in your Netlify dashboard

# Core Application
NODE_ENV=production
VITE_APP_URL=https://instantiate.dev
VITE_API_URL=https://api.instantiate.dev

# Database
DATABASE_URL=your_production_database_url

# AI Chat Service
GROQ_API_KEY=your_groq_api_key

# Security
JWT_SECRET=your_jwt_secret_key
ENCRYPTION_KEY=your_encryption_key

# Analytics (Optional)
GOOGLE_ANALYTICS_ID=your_ga_id
MIXPANEL_TOKEN=your_mixpanel_token

# Monitoring (Optional)
SENTRY_DSN=your_sentry_dsn
UPTIME_ROBOT_API_KEY=your_uptime_robot_key
